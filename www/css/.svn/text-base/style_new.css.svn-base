  width: 274px;
  overflow: visible;
}
.b_news_edit .form_list li {
	overflow: visible;
	text-align: left;
	min-height: 47px;
}
.b_news_edit .jquery-selectbox{
	width: 262px;
}
.b_news_edit .jquery-selectbox .jquery-selectbox-list{
  min-width: 268px;
}
.b_news_edit .form-textarea{
	margin-bottom: 5px;
	height: 280px;
}
.b_news_edit .b_photo_block{
	background: white;
	overflow: hidden;
	padding: 14px 10px;
}
.b_news_edit .b_photo_block .b_photo{
	border: none;
	width: 158px;
	height: 120px;
}
.b_news_edit .b_photo_block .b_nav{
	float: right;
}
.b_news_edit .b_photo_block .btn{
	padding: 4px 3px 5px;
	display: block;
	text-align: center;
	width: 80px;
	font: 12px Arial;
	color: #3388cd;
	font-weight: normal;
	margin-bottom: 5px;
	background: url(../images/main_menu_btn.png) repeat-x 0 50%;
