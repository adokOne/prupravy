	text-decoration: none;
}
.b_news_edit .b_photo_block .btn_add{
	margin-top: 45px;
}
.b_news_edit .b_photo_block .btn_edit{
	margin-top: 35px;
}
.b_news_edit .b_photo_block .btn.red{
	background: url(../images/bg_submit_red.png) repeat-x 0 50%;
	color: white;
}