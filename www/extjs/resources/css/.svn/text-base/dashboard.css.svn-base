.ext-ux-uploaddialog-state {
		text-align: center; background-position: center center; background-repeat: no-repeat;
}

.ext-ux-uploaddialog-state-0 { background-image: url('../images/default/uncheck.gif'); }

.ext-ux-uploaddialog-state-1 { background-image: url('../images/default/check.gif'); }

.ext-ux-uploaddialog-state-2 { background-image: url('../images/default/failed.gif');}
 
.ext-ux-uploaddialog-state-3 { background-image: url('../images/default/file-uploading.gif'); }

