#loading {
    position: absolute;
    left: 45%;
    top: 40%;
    padding: 2px;
    z-index: 20001;
    height: auto;
    width: 140px;
}

#loading img {
    margin-bottom: 5px;
    margin-top: 5px;
}

#loading .loading-indicator {
    background: white;
    border: solid 1px #777;
    color:#555;
    font: bold 13px tahoma,arial,helvetica;
    padding: 10px;
    margin: 0;
    text-align: center;
    height: auto;
} 


